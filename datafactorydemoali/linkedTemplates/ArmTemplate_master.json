{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactorydemoali"
		},
		"linkedservice_AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedservice_AzureBlobStorage'"
		},
		"linkedservice_AzureBlobStorage_lookupActivity_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedservice_AzureBlobStorage_lookupActivity'"
		},
		"linkedservice_AzureSqlDatabase_lookupActivity_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedservice_AzureSqlDatabase_lookupActivity'"
		},
		"linkedservice_dbparameterization_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedservice_dbparameterization'"
		},
		"linkedservice_general_parametrization_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedservice_general_parametrization'",
			"defaultValue": "Integrated Security=False;Data Source=@{linkedService().serverName};Initial Catalog=@{linkedService().dbName};User ID=@{linkedService().userName};Password=@{linkedService().password}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactorydemoali_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linkedservice_AzureBlobStorage_connectionString": {
						"value": "[parameters('linkedservice_AzureBlobStorage_connectionString')]"
					},
					"linkedservice_AzureBlobStorage_lookupActivity_connectionString": {
						"value": "[parameters('linkedservice_AzureBlobStorage_lookupActivity_connectionString')]"
					},
					"linkedservice_AzureSqlDatabase_lookupActivity_connectionString": {
						"value": "[parameters('linkedservice_AzureSqlDatabase_lookupActivity_connectionString')]"
					},
					"linkedservice_dbparameterization_connectionString": {
						"value": "[parameters('linkedservice_dbparameterization_connectionString')]"
					},
					"linkedservice_general_parametrization_connectionString": {
						"value": "[parameters('linkedservice_general_parametrization_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "datafactorydemoali_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datafactorydemoali_ArmTemplate_0')]"
			]
		},
		{
			"name": "datafactorydemoali_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datafactorydemoali_ArmTemplate_1')]"
			]
		},
		{
			"name": "datafactorydemoali_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datafactorydemoali_ArmTemplate_2')]"
			]
		}
	]
}